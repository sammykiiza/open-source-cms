{
    "name": "sammykiiza/open-source-cms",
    "description": "A PHP open source content management system",
    "type": "project",
    "require-dev": {
        "phpunit/phpunit": "10.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\App\\": "Tests/"
        }
    },
    "authors": [
        {
            "name": "sammykiiza",
            "email": "sammykiiza@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "doctrine/orm": "^2.11.0",
        "doctrine/dbal": "^3.2",
        "doctrine/annotations": "1.13.2",
        "symfony/yaml": "^5.4",
        "symfony/cache": "^5.4",
        "ramsey/uuid": "5.x-dev",
        "web-token/jwt-core": "v3.0.x-dev",
        "web-token/jwt-key-mgmt": "*",
        "web-token/jwt-checker": "*",
        "web-token/jwt-signature": "*",
        "web-token/jwt-signature-algorithm-hmac": "*",
        "vlucas/phpdotenv": "5.5.x-dev"
    }
}
